###################################################################################################
#Вычисление относительного пути от места запуска к каталогу, содержащему этот makefile
###################################################################################################
ROOT := $(patsubst %/,%,$(dir $(word $(words $(MAKEFILE_LIST)),$(MAKEFILE_LIST))))

###################################################################################################
#Подключение скрипта сборки
###################################################################################################
include $(ROOT)/build/make-main.mak
