<?xml version="1.0" encoding = "windows-1251"?>
<Configuration>
  <FileSystem Paths="data"/>
  <Log>
    <Console Filters="engine.*"/>
    <File FileName="/io/stdout" Filters="engine.*" Flush="1">
      <OutputFormat 
        Replacement="{log}: {message}" 
        SortOrder="1"
      >
        Filter="*" 
      </OutputFormat>
    </File>
  </Log>
  <Shell Interpreter="lua" Sources="shell_scripts/*" Libraries="Math SceneGraph" Execute="main ()"/>
</Configuration>
