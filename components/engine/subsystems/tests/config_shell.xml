<?xml version="1.0" encoding = "windows-1251"?>
<Configuration>
  <Log>
    <Console Filters="engine.*"/>
    <File FileName="/io/stdout" Filters="engine.*" Flush="1">
      <OutputFormat 
        Filter="*" 
        Replacement="{log}: {message}" 
        SortOrder="1"
      />
    </File>
  </Log>
  <Shell Interpreter="lua" Sources="data/shell_scripts/*" Libraries="Math SceneGraph" Execute="main ()"/>
</Configuration>
